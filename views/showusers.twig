{% extends "dashboard.twig" %}
{% block content1 %}
   <h1 class="h2">Usuarios</h1>
{% endblock %}

{% block content %}



<div class="table-responsive">
  <table class="table table-striped table-sm">
        <thead>
          <tr>
            <th>Email</th>
            <th>Nivel </th>
            <th>Editar </th>
            <th>Eliminar </th>
          </tr>
        </thead>
        <tbody>
            {% for user in users %}
              <tr>
                <td>{{ user.email }}</td>
                <td>{{ user.type}}</td>
                <td> <a href="/ENS_TELEMATICA/user/edit?id={{user.id}}">Editar</a> </td>
                <td> <a href="/ENS_TELEMATICA/user/delete?id={{user.id}}">Eliminar</a> </td>
              </tr>
             {% endfor %}
        </tbody>
      </table>
    </div>






{% endblock %}
